		<![CDATA[
		SELECT 
		A001.*, 
		A002.clientNameInSystem, 
		A003.clientState, 
		A003.dealInfo, 
		A003.representative, 
		A004.name 
		FROM GUMclient AS A001 
		INNER JOIN GUMcompany AS A002 ON A001.codeNumber = A002.codeNumber 
		LEFT OUTER JOIN GUTclientBase AS A003 ON A001.codeNumber = A003.codeNumber AND A001.officeCode = A003.officeCode AND A003.status = '1' 
		LEFT OUTER JOIN GUMstaff AS A004 ON A003.representative = A004.staffID AND A004.status = '1' 
		WHERE A001.status = '?' AND A002.status = '?' 
		]]>
